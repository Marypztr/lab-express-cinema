<section class="detail">
    <div>
        <img src="{{this.image}}" alt="">
    </div>
    <div class= "detailText">
       <a href="/movies"> <p>Go back</p> </a>
        <h2>{{this.title}}</h2>
        <h3>Director: {{this.director}}</h3>
        <h4>Stars</h4>
        <ul>
            {{#each stars}}
            <li>
                {{this}}
            </li>
            {{/each}}
        </ul>
        <p>{{this.description}}</p>
        <p>{{this.showtimes}}</p>
    </div>
</section>